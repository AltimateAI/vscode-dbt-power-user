{
	"extends": "./tsconfig.browser.json",
	"compilerOptions": {
		"rootDir": "webviews",
		"jsx": "react",
		"esModuleInterop": true,
		"isolatedModules": false,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"target": "es2019",
		"useDefineForClassFields": true
	},
}