          "group": "navigation@5"
        },
        {
          "command": "dbtPowerUser.sqlQuickPick",
          "when": "resourceLangId =~ /^sql$|^jinja-sql$/",
          "group": "navigation@1"
        },
        {
          "command": "dbtPowerUser.sqlToModel",
          "when": "resourceLangId =~ /^sql$|^jinja-sql$/",
          "group": "navigation@6"
        }
      ],
      "editor/context": [
        {
          "submenu": "dbtPowerUser.datapilotSubmenu",
          "group": "datapilot@1",
          "when": "resourceLangId =~ /^sql$|^jinja-sql$/"
        },
        {
